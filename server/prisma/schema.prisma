// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// create table
model Feedback {
  id String @id @default(uuid()) //primary key id unico universal

  type       String
  comment    String
  screenshot String?

  @@map("feedbacks")
}
